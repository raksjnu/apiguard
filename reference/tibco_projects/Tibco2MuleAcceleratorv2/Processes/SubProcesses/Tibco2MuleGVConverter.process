<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pfx6="http://www.tibco.com/xmlns/repo/types/2002" xmlns:pfx="http://www.mulesoft.org/schema/mule/core" xmlns:sap="http://www.mulesoft.org/schema/mule/sap" xmlns:pfx2="http://www.tibco.com/schemas/tibco2mulePOC/Raks/report.xsd" xmlns:ns="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:pfx3="http://ibm.com/raks/tibco2mule/accelerator/Resources/Schema/Tibco/All.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:ns1="http://www.tibco.com/Resources/Schema/FlowSchema/FlowSchema.xsd" xmlns:ns4="http://ibm.com/raks/tibco2mule/accelerator/Resources/Schema/Tibco/ProcessInfo.xsd" xmlns:ns3="http://www.tibco.com/pe/DeployedVarsType" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns6="http://www.tibco.com/schemas/Tibco2MuleAccelerator/Resources/Schema/Tibco/FileInfo.xsd" xmlns:ns5="http://www.tibco.com/schemas/Tibco2MuleAccelerator/Resources/Schema/Tibco/ProjectInfo.xsd" xmlns:ns8="http://ibm.com/raks/tibco2mule/accelerator/Resources/Schema/Checklist.xsd" xmlns:dw="http://www.mulesoft.org/schema/mule/ee/dw" xmlns:file="http://www.mulesoft.org/schema/mule/file" xmlns:ns7="http://ibm.com/raks/tibco2mule/accelerator/Resources/Schema/Tibco/GVInfo.xsd" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns12="http://www.tibco.com/namespaces/tnt/plugins/file/v2" xmlns:http="http://www.mulesoft.org/schema/mule/http" xmlns:doc="http://www.mulesoft.org/schema/mule/documentation" xmlns:db="http://www.mulesoft.org/schema/mule/db">
    <xsd:import namespace="http://www.w3.org/2001/XMLSchema" schemaLocation="/ProcessDefinitionParsing/Schema.xsd"/>
    <xsd:import namespace="http://www.tibco.com/schemas/tibco2mulePOC/Raks/report.xsd" schemaLocation="/Raks/report.xsd"/>
    <xsd:import namespace="http://www.mulesoft.org/schema/mule/sap" schemaLocation="/Raks/sap/mule-sap.xsd"/>
    <xsd:import namespace="http://www.tibco.com/xmlns/repo/types/2002" schemaLocation="/Resources/Schema/Tibco/GVDetails.xsd"/>
    <xsd:import namespace="http://ibm.com/raks/tibco2mule/accelerator/Resources/Schema/Tibco/All.xsd" schemaLocation="/Resources/Schema/Tibco/All.xsd"/>
    <xsd:import namespace="http://www.mulesoft.org/schema/mule/core"/>
    <pd:name>Processes/SubProcesses/Tibco2MuleGVConverter.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx3:root"/>
    <pd:startX>33</pd:startX>
    <pd:startY>40</pd:startY>
    <pd:returnBindings>
        <pfx3:root>
            <xsl:for-each select="$Start/pfx3:root/ns5:ProjectInfo">
                <ns5:ProjectInfo>
                    <xsl:if test="ns5:name">
                        <ns5:name>
                            <xsl:value-of select="ns5:name"/>
                        </ns5:name>
                    </xsl:if>
                    <xsl:if test="ns5:path">
                        <ns5:path>
                            <xsl:value-of select="ns5:path"/>
                        </ns5:path>
                    </xsl:if>
                    <xsl:if test="ns5:mulePath">
                        <ns5:mulePath>
                            <xsl:value-of select="ns5:mulePath"/>
                        </ns5:mulePath>
                    </xsl:if>
                </ns5:ProjectInfo>
            </xsl:for-each>
            <xsl:copy-of select="$ParseAnyXML/pfx3:root/ns7:GVInfo"/>
        </pfx3:root>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx3:root"/>
    <pd:endX>927</pd:endX>
    <pd:endY>42</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables>
        <Variable0>
            <xsd:element name="root">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="param" type="xsd:string" minOccurs="0"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </Variable0>
    </pd:processVariables>
    <pd:targetNamespace>http://xmlns.example.com/1573683890160</pd:targetNamespace>
    <pd:activity name="Read File">
        <pd:type>com.tibco.plugin.file.FileReadActivity</pd:type>
        <pd:resourceType>ae.activities.FileReadActivity</pd:resourceType>
        <pd:x>113</pd:x>
        <pd:y>40</pd:y>
        <config>
            <encoding>text</encoding>
        </config>
        <pd:inputBindings>
            <ns12:ReadActivityInputClass>
                <fileName>
                    <xsl:value-of select="$Start/pfx3:root/ns6:FileInfo[1]/ns6:filePath"/>
                </fileName>
            </ns12:ReadActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="GVFilePathName">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>202</pd:x>
        <pd:y>41</pd:y>
        <config>
            <element>
                <xsd:element name="root">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="GVPathName" type="xsd:string"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </element>
        </config>
        <pd:inputBindings>
            <root>
                <GVPathName>
                    <xsl:value-of select="concat(translate(substring-after(tib:substring-before-last(substring-after($Read-File/ns2:ReadActivityOutputTextClass/fileInfo/fullName, $Start/pfx3:root/ns5:ProjectInfo[1]/ns5:name), &quot;\&quot;), &quot;\&quot;), &quot;\&quot;, &quot;.&quot;),&quot;.&quot;)"/>
                </GVPathName>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Render Data">
        <pd:type>com.tibco.plugin.parse.RenderActivity</pd:type>
        <pd:resourceType>ae.activities.RenderActivity</pd:resourceType>
        <pd:x>710</pd:x>
        <pd:y>43</pd:y>
        <config>
            <ParseSharedConfig>/Resources/DataFormat/PropFileDataFormat.sharedparse</ParseSharedConfig>
        </config>
        <pd:inputBindings>
            <Rows>
                <xsl:for-each select="$accumulatedGV/AccumulatedOutput/pfx3:root/ns7:GVInfo">
                    <NVPair>
                        <Name>
                            <xsl:value-of select="{--no need to append defaultVars. for every GV property in Mule so removing it--}&#xA;concat($GVFilePathName/root/GVPathName,current()/ns7:name,&quot; = &quot;,current()/ns7:value)"/>
                        </Name>
                    </NVPair>
                </xsl:for-each>
            </Rows>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="WritePropertiesFile">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>814</pd:x>
        <pd:y>44</pd:y>
        <config>
            <processName>/Processes/Common/FileWriting.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <FilePath>
                    <xsl:value-of select="concat($Start/pfx3:root/ns5:ProjectInfo[1]/ns5:mulePath,$_globalVariables/ns3:GlobalVariables/Tibco2MuleAccelerator/Output/muleFolderResources,$_globalVariables/ns3:GlobalVariables/Tibco2MuleAccelerator/Output/muleConfigFileName)"/>
                </FilePath>
                <FileContent>
                    <xsl:value-of select="$Render-Data/Text"/>
                </FileContent>
                <Append>
                    <xsl:value-of select="true()"/>
                </Append>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="ParseAnyXML">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>307</pd:x>
        <pd:y>42</pd:y>
        <config>
            <processName>/Processes/Common/ParseAnyXML.process</processName>
        </config>
        <pd:inputBindings>
            <pfx3:root>
                <ns6:FileInfo>
                    <ns6:filePath>
                        <xsl:value-of select="$Read-File/ns2:ReadActivityOutputTextClass/fileInfo/fullName"/>
                    </ns6:filePath>
                    <ns6:fileContent>
                        <xsl:value-of select="$Read-File/ns2:ReadActivityOutputTextClass/fileContent/textContent"/>
                    </ns6:fileContent>
                    <ns6:fileType>
                        <xsl:value-of select="'substvar'"/>
                    </ns6:fileType>
                </ns6:FileInfo>
            </pfx3:root>
        </pd:inputBindings>
    </pd:activity>
    <pd:group name="DecryptPasswordGroup">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:description>to decrypt the password set $_globalVariables/ns3:GlobalVariables/Tibco2MuleAccelerator/Extracter/GV/DecryptPassword='Yes'</pd:description>
        <pd:x>361</pd:x>
        <pd:y>1</pd:y>
        <pd:width>290</pd:width>
        <pd:height>197</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>inputLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:over>$ParseAnyXML/pfx3:root/ns7:GVInfo</pd:over>
            <pd:iterationElementSlot>currentGV</pd:iterationElementSlot>
            <pd:indexSlot>i</pd:indexSlot>
            <pd:outSlot>accumulatedGV</pd:outSlot>
            <pd:activityOutputName>ParsedGV</pd:activityOutputName>
            <pd:accumulateOutput>true</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>EncryptionDecryption</pd:to>
            <pd:xpathDescription>PasswrodType</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>xpath</pd:conditionType>
            <pd:xpath>$currentGV/ns7:GVInfo/ns7:type='Password' and $_globalVariables/ns3:GlobalVariables/Tibco2MuleAccelerator/Filters/DecryptPassword='Yes'</pd:xpath>
        </pd:transition>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Null</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>otherwise</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>ParsedGV</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>EncryptionDecryption</pd:from>
            <pd:to>ParsedGV</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Null</pd:from>
            <pd:to>ParsedGV</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="EncryptionDecryption">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>478</pd:x>
            <pd:y>56</pd:y>
            <config>
                <processName>/Processes/Common/EncryptionDecryption.process</processName>
            </config>
            <pd:inputBindings>
                <root>
                    <xsl:if test="$currentGV/ns7:GVInfo/ns7:value">
                        <StringToDecrypt>
                            <xsl:value-of select="$currentGV/ns7:GVInfo/ns7:value"/>
                        </StringToDecrypt>
                    </xsl:if>
                </root>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="ParsedGV">
            <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
            <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
            <pd:x>575</pd:x>
            <pd:y>105</pd:y>
            <config>
                <element ref="pfx3:root"/>
            </config>
            <pd:inputBindings>
                <pfx3:root>
                    <ns7:GVInfo>
                        <ns7:name>
                            <xsl:value-of select="$currentGV/ns7:GVInfo/ns7:name"/>
                        </ns7:name>
                        <xsl:if test="if (string-length($EncryptionDecryption/root/DecryptedValue)>0) then&#xA;$EncryptionDecryption/root/DecryptedValue else&#xA;$currentGV/ns7:GVInfo/ns7:value">
                            <ns7:value>
                                <xsl:value-of select="if (string-length($EncryptionDecryption/root/DecryptedValue)>0) then&#xA;$EncryptionDecryption/root/DecryptedValue else&#xA;$currentGV/ns7:GVInfo/ns7:value"/>
                            </ns7:value>
                        </xsl:if>
                        <xsl:if test="$currentGV/ns7:GVInfo/ns7:deploymentSettable">
                            <ns7:deploymentSettable>
                                <xsl:value-of select="$currentGV/ns7:GVInfo/ns7:deploymentSettable"/>
                            </ns7:deploymentSettable>
                        </xsl:if>
                        <xsl:if test="$currentGV/ns7:GVInfo/ns7:serviceSettable">
                            <ns7:serviceSettable>
                                <xsl:value-of select="$currentGV/ns7:GVInfo/ns7:serviceSettable"/>
                            </ns7:serviceSettable>
                        </xsl:if>
                        <xsl:if test="$currentGV/ns7:GVInfo/ns7:type">
                            <ns7:type>
                                <xsl:value-of select="$currentGV/ns7:GVInfo/ns7:type"/>
                            </ns7:type>
                        </xsl:if>
                        <xsl:if test="$currentGV/ns7:GVInfo/ns7:modTime">
                            <ns7:modTime>
                                <xsl:value-of select="$currentGV/ns7:GVInfo/ns7:modTime"/>
                            </ns7:modTime>
                        </xsl:if>
                    </ns7:GVInfo>
                </pfx3:root>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Null">
            <pd:type>com.tibco.plugin.timer.NullActivity</pd:type>
            <pd:resourceType>ae.activities.null</pd:resourceType>
            <pd:x>479</pd:x>
            <pd:y>142</pd:y>
            <config/>
            <pd:inputBindings/>
        </pd:activity>
    </pd:group>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Read File</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Read File</pd:from>
        <pd:to>GVFilePathName</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Render Data</pd:from>
        <pd:to>WritePropertiesFile</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>WritePropertiesFile</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>GVFilePathName</pd:from>
        <pd:to>ParseAnyXML</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>ParseAnyXML</pd:from>
        <pd:to>DecryptPasswordGroup</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>DecryptPasswordGroup</pd:from>
        <pd:to>Render Data</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>