<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pfx="http://www.mulesoft.org/schema/mule/core" xmlns:sap="http://www.mulesoft.org/schema/mule/sap" xmlns:pfx2="http://www.tibco.com/schemas/tibco2mulePOC/Raks/report.xsd" xmlns:ns="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:pfx3="http://www.tibco.com/Resources/Schema/FlowSchema/FlowSchema.xsd" xmlns:pfx4="http://ibm.com/raks/tibco2mule/accelerator/Resources/Schema/Tibco/All.xsd" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:ns1="http://www.tibco.com/schemas/Tibco2MuleAccelerator/Resources/Schema/Tibco/FileInfo.xsd" xmlns:ns4="http://www.tibco.com/schemas/Tibco2MuleAccelerator/Resources/Schema/Tibco/ProjectInfo.xsd" xmlns:ns3="http://www.tibco.com/pe/DeployedVarsType" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns6="http://ibm.com/raks/tibco2mule/accelerator/Resources/Schema/Checklist.xsd" xmlns:ns5="http://ibm.com/raks/tibco2mule/accelerator/Resources/Schema/Tibco/ProcessInfo.xsd" xmlns:ns8="http://ibm.com/raks/tibco2mule/accelerator/Resources/Schema/ChecklistSharedVar.xsd" xmlns:dw="http://www.mulesoft.org/schema/mule/ee/dw" xmlns:file="http://www.mulesoft.org/schema/mule/file" xmlns:ns7="http://ibm.com/raks/tibco2mule/accelerator/Resources/Schema/Tibco/GVInfo.xsd" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns9="http://ibm.com/raks/tibco2mule/accelerator/Resources/Schema/Tibco/NameValue.xsd" xmlns:http="http://www.mulesoft.org/schema/mule/http" xmlns:doc="http://www.mulesoft.org/schema/mule/documentation" xmlns:db="http://www.mulesoft.org/schema/mule/db">
    <xsd:import namespace="http://www.w3.org/2001/XMLSchema" schemaLocation="/ProcessDefinitionParsing/Schema.xsd"/>
    <xsd:import namespace="http://www.mulesoft.org/schema/mule/core" schemaLocation="/Raks/Mule.xsd"/>
    <xsd:import namespace="http://www.tibco.com/schemas/tibco2mulePOC/Raks/report.xsd" schemaLocation="/Raks/report.xsd"/>
    <xsd:import namespace="http://www.mulesoft.org/schema/mule/sap" schemaLocation="/Raks/sap/mule-sap.xsd"/>
    <xsd:import namespace="http://www.tibco.com/Resources/Schema/FlowSchema/FlowSchema.xsd" schemaLocation="/Resources/Schema/Tibco/FlowSchema.xsd"/>
    <xsd:import namespace="http://ibm.com/raks/tibco2mule/accelerator/Resources/Schema/Tibco/All.xsd" schemaLocation="/Resources/Schema/Tibco/All.xsd"/>
    <xsd:import namespace="http://ibm.com/raks/tibco2mule/accelerator/Resources/Schema/Tibco/NameValue.xsd" schemaLocation="/Resources/Schema/Tibco/NameValue.xsd"/>
    <pd:name>Processes/Common/ParseAnyXML.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx4:root"/>
    <pd:startX>27</pd:startX>
    <pd:startY>71</pd:startY>
    <pd:returnBindings>
        <pfx4:root>
            <xsl:if test="$Start/pfx4:root/ns1:FileInfo/ns1:fileType='substvar'">
                <xsl:for-each select="$TIBCO_XML/child::*/child::*/*">
                    <ns7:GVInfo>
                        <xsl:if test="current()/*[1]">
                            <ns7:name>
                                <xsl:value-of select="current()/*[1]"/>
                            </ns7:name>
                        </xsl:if>
                        <xsl:if test="current()/*[2]">
                            <ns7:value>
                                <xsl:value-of select="current()/*[2]"/>
                            </ns7:value>
                        </xsl:if>
                        <xsl:if test="current()/*[3]">
                            <ns7:deploymentSettable>
                                <xsl:value-of select="current()/*[3]"/>
                            </ns7:deploymentSettable>
                        </xsl:if>
                        <xsl:if test="current()/*[4]">
                            <ns7:serviceSettable>
                                <xsl:value-of select="current()/*[4]"/>
                            </ns7:serviceSettable>
                        </xsl:if>
                        <xsl:if test="current()/*[5]">
                            <ns7:type>
                                <xsl:value-of select="current()/*[5]"/>
                            </ns7:type>
                        </xsl:if>
                        <xsl:if test="current()/*[6]">
                            <ns7:modTime>
                                <xsl:value-of select="current()/*[6]"/>
                            </ns7:modTime>
                        </xsl:if>
                    </ns7:GVInfo>
                </xsl:for-each>
            </xsl:if>
            <ns5:ProcessInfo>
                <xsl:for-each select="$TIBCO_ActivityDetails-Final/root/Activities">
                    <ns5:activity>
                        <xsl:if test="process">
                            <ns5:processName>
                                <xsl:value-of select="process"/>
                            </ns5:processName>
                        </xsl:if>
                        <xsl:if test="name">
                            <ns5:name>
                                <xsl:value-of select="name"/>
                            </ns5:name>
                        </xsl:if>
                        <xsl:if test="type">
                            <ns5:type>
                                <xsl:value-of select="type"/>
                            </ns5:type>
                        </xsl:if>
                        <xsl:if test="resourceType">
                            <ns5:resourceType>
                                <xsl:value-of select="resourceType"/>
                            </ns5:resourceType>
                        </xsl:if>
                        <xsl:if test="config">
                            <ns5:config>
                                <xsl:value-of select="config"/>
                            </ns5:config>
                        </xsl:if>
                        <xsl:if test="InputBinding">
                            <ns5:inputBinding>
                                <xsl:value-of select="InputBinding"/>
                            </ns5:inputBinding>
                        </xsl:if>
                        <xsl:if test="Starter">
                            <ns5:starter>
                                <xsl:value-of select="Starter"/>
                            </ns5:starter>
                        </xsl:if>
                        <ns5:from>
                            <xsl:value-of select="from"/>
                        </ns5:from>
                        <ns5:to>
                            <xsl:value-of select="to"/>
                        </ns5:to>
                        <xsl:if test="groupName">
                            <ns5:groupName>
                                <xsl:value-of select="groupName"/>
                            </ns5:groupName>
                        </xsl:if>
                        <xsl:if test="rawConfig">
                            <ns5:rawConfig>
                                <xsl:value-of select="rawConfig"/>
                            </ns5:rawConfig>
                        </xsl:if>
                        <xsl:if test="rawInputBinding">
                            <ns5:rawInputBinding>
                                <xsl:value-of select="rawInputBinding"/>
                            </ns5:rawInputBinding>
                        </xsl:if>
                        <xsl:if test="current()/PosX">
                            <ns5:posX>
                                <xsl:value-of select="current()/PosX"/>
                            </ns5:posX>
                        </xsl:if>
                        <xsl:if test="current()/PosY">
                            <ns5:posY>
                                <xsl:value-of select="current()/PosY"/>
                            </ns5:posY>
                        </xsl:if>
                    </ns5:activity>
                </xsl:for-each>
            </ns5:ProcessInfo>
            <xsl:if test="{--CheckList1&#x9;Checklist_GenerateError--}&#xA;$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='1']/ns8:Enabled= 'Yes'">
                <xsl:for-each select="$TIBCO_ActivityDetails/root/Activities[type='GenerateErrorActivity']">
                    <ns6:ChecklistValidation>
                        <xsl:if test="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='1']/ns8:ChecklistNumber">
                            <ns6:ChecklistNumber>
                                <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='1']/ns8:ChecklistNumber"/>
                            </ns6:ChecklistNumber>
                        </xsl:if>
                        <xsl:if test="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='1']/ns8:ChecklistName">
                            <ns6:ChecklistName>
                                <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='1']/ns8:ChecklistName"/>
                            </ns6:ChecklistName>
                        </xsl:if>
                        <ns6:Checklist>
                            <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='1']/ns8:ChecklistDetail"/>
                        </ns6:Checklist>
                        <ns6:Validation>
                            <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='1']/ns8:Severity"/>
                        </ns6:Validation>
                        <ns6:Remarks>
                            <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='1']/ns8:Remarks"/>
                        </ns6:Remarks>
                        <ns6:Process>
                            <xsl:value-of select="$Start/pfx4:root/ns1:FileInfo[1]/ns1:filePath"/>
                        </ns6:Process>
                        <ns6:Activity>
                            <xsl:value-of select="concat(../Transitions[to=current()/name]/from, '-->', current()/name)"/>
                        </ns6:Activity>
                        <ns6:ReviewAccepted>
                            <xsl:value-of select="&quot;&quot;"/>
                        </ns6:ReviewAccepted>
                        <ns6:DeveloperComment>
                            <xsl:value-of select="&quot;&quot;"/>
                        </ns6:DeveloperComment>
                    </ns6:ChecklistValidation>
                </xsl:for-each>
            </xsl:if>
            <xsl:if test="{--CheckList2&#x9;Checklist_ProcessVariable--}&#xA;$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='2']/ns8:Enabled= 'Yes'&#xA;and $TIBCO_ActivityDetails/root/Activities[type='AssignActivity']&#xA;and &#xA;contains(tib:render-xml($TIBCO_ActivityDetails/root/processVariables[1], 1,0), substring-before(substring-after($TIBCO_ActivityDetails/root/Activities[type='AssignActivity']/rawConfig, '&lt;variableName>'), '&lt;/variableName>'))">
                <xsl:for-each select="$TIBCO_ActivityDetails/root/Activities[type='AssignActivity']">
                    <ns6:ChecklistValidation>
                        <xsl:if test="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='2']/ns8:ChecklistNumber">
                            <ns6:ChecklistNumber>
                                <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='2']/ns8:ChecklistNumber"/>
                            </ns6:ChecklistNumber>
                        </xsl:if>
                        <xsl:if test="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='2']/ns8:ChecklistName">
                            <ns6:ChecklistName>
                                <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='2']/ns8:ChecklistName"/>
                            </ns6:ChecklistName>
                        </xsl:if>
                        <ns6:Checklist>
                            <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='2']/ns8:ChecklistDetail"/>
                        </ns6:Checklist>
                        <ns6:Validation>
                            <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='2']/ns8:Severity"/>
                        </ns6:Validation>
                        <ns6:Remarks>
                            <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='2']/ns8:Remarks"/>
                        </ns6:Remarks>
                        <ns6:Process>
                            <xsl:value-of select="$Start/pfx4:root/ns1:FileInfo[1]/ns1:filePath"/>
                        </ns6:Process>
                        <ns6:Activity>
                            <xsl:value-of select="current()/name"/>
                        </ns6:Activity>
                        <ns6:ReviewAccepted>
                            <xsl:value-of select="&quot;&quot;"/>
                        </ns6:ReviewAccepted>
                        <ns6:DeveloperComment>
                            <xsl:value-of select="&quot;&quot;"/>
                        </ns6:DeveloperComment>
                    </ns6:ChecklistValidation>
                </xsl:for-each>
            </xsl:if>
            <xsl:if test="{--CheckList3&#x9;Checklist_JMSHeaderProperty--}&#xA;$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='3']/ns8:Enabled= 'Yes'">
                <xsl:for-each select="$TIBCO_ActivityDetails/root/Activities[(type='JMSQueueSendActivity' or type='JMSTopicPublishActivity' or type='JMSTopicRequestReplyActivity') and&#xA;( not(contains(rawInputBinding, 'DynamicProperties')) or not(contains(rawConfig, 'ApplicationProperties')))&#xA;]">
                    <ns6:ChecklistValidation>
                        <xsl:if test="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='3']/ns8:ChecklistNumber">
                            <ns6:ChecklistNumber>
                                <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='3']/ns8:ChecklistNumber"/>
                            </ns6:ChecklistNumber>
                        </xsl:if>
                        <xsl:if test="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='3']/ns8:ChecklistName">
                            <ns6:ChecklistName>
                                <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='3']/ns8:ChecklistName"/>
                            </ns6:ChecklistName>
                        </xsl:if>
                        <ns6:Checklist>
                            <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='3']/ns8:ChecklistDetail"/>
                        </ns6:Checklist>
                        <ns6:Validation>
                            <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='3']/ns8:Severity"/>
                        </ns6:Validation>
                        <ns6:Remarks>
                            <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='3']/ns8:Remarks"/>
                        </ns6:Remarks>
                        <ns6:Process>
                            <xsl:value-of select="$Start/pfx4:root/ns1:FileInfo[1]/ns1:filePath"/>
                        </ns6:Process>
                        <ns6:Activity>
                            <xsl:value-of select="current()/name"/>
                        </ns6:Activity>
                        <ns6:ReviewAccepted>
                            <xsl:value-of select="&quot;&quot;"/>
                        </ns6:ReviewAccepted>
                        <ns6:DeveloperComment>
                            <xsl:value-of select="&quot;&quot;"/>
                        </ns6:DeveloperComment>
                    </ns6:ChecklistValidation>
                </xsl:for-each>
            </xsl:if>
            <xsl:if test="{--CheckList4&#x9;Checklist_JMSPreserveProperty--}&#xA;$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='4']/ns8:Enabled= 'Yes'">
                <xsl:for-each select="$TIBCO_ActivityDetails/root/Activities[(type='JMSQueueSendActivity' or type='JMSTopicPublishActivity' or type='JMSTopicRequestReplyActivity') and&#xA;( not(contains(rawInputBinding, 'JMS_TIBCO_PRESERVE_UNDELIVERED')))&#xA;]">
                    <ns6:ChecklistValidation>
                        <xsl:if test="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='4']/ns8:ChecklistNumber">
                            <ns6:ChecklistNumber>
                                <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='4']/ns8:ChecklistNumber"/>
                            </ns6:ChecklistNumber>
                        </xsl:if>
                        <xsl:if test="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='4']/ns8:ChecklistName">
                            <ns6:ChecklistName>
                                <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='4']/ns8:ChecklistName"/>
                            </ns6:ChecklistName>
                        </xsl:if>
                        <ns6:Checklist>
                            <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='4']/ns8:ChecklistDetail"/>
                        </ns6:Checklist>
                        <ns6:Validation>
                            <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='4']/ns8:Severity"/>
                        </ns6:Validation>
                        <ns6:Remarks>
                            <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='4']/ns8:Remarks"/>
                        </ns6:Remarks>
                        <ns6:Process>
                            <xsl:value-of select="$Start/pfx4:root/ns1:FileInfo[1]/ns1:filePath"/>
                        </ns6:Process>
                        <ns6:Activity>
                            <xsl:value-of select="current()/name"/>
                        </ns6:Activity>
                        <ns6:ReviewAccepted>
                            <xsl:value-of select="&quot;&quot;"/>
                        </ns6:ReviewAccepted>
                        <ns6:DeveloperComment>
                            <xsl:value-of select="&quot;&quot;"/>
                        </ns6:DeveloperComment>
                    </ns6:ChecklistValidation>
                </xsl:for-each>
            </xsl:if>
            <xsl:if test="{--CheckList5&#x9;Checklist_DurableTopicSubscriber--}&#xA;$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='5']/ns8:Enabled= 'Yes'">
                <xsl:for-each select="$TIBCO_ActivityDetails/root/Starter[type='JMSTopicEventSource'  and&#xA;contains(rawConfig, '&lt;durable>true&lt;/durable>')&#xA;]">
                    <ns6:ChecklistValidation>
                        <xsl:if test="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='5']/ns8:ChecklistNumber">
                            <ns6:ChecklistNumber>
                                <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='5']/ns8:ChecklistNumber"/>
                            </ns6:ChecklistNumber>
                        </xsl:if>
                        <xsl:if test="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='5']/ns8:ChecklistName">
                            <ns6:ChecklistName>
                                <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='5']/ns8:ChecklistName"/>
                            </ns6:ChecklistName>
                        </xsl:if>
                        <ns6:Checklist>
                            <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='5']/ns8:ChecklistDetail"/>
                        </ns6:Checklist>
                        <ns6:Validation>
                            <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='5']/ns8:Severity"/>
                        </ns6:Validation>
                        <ns6:Remarks>
                            <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='5']/ns8:Remarks"/>
                        </ns6:Remarks>
                        <ns6:Process>
                            <xsl:value-of select="$Start/pfx4:root/ns1:FileInfo[1]/ns1:filePath"/>
                        </ns6:Process>
                        <ns6:Activity>
                            <xsl:value-of select="current()/name"/>
                        </ns6:Activity>
                        <ns6:ReviewAccepted>
                            <xsl:value-of select="&quot;&quot;"/>
                        </ns6:ReviewAccepted>
                        <ns6:DeveloperComment>
                            <xsl:value-of select="&quot;&quot;"/>
                        </ns6:DeveloperComment>
                    </ns6:ChecklistValidation>
                </xsl:for-each>
            </xsl:if>
            <xsl:if test="{--CheckList6&#x9;Checklist_ConfirmActivity--}&#xA;$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='6']/ns8:Enabled= 'Yes'&#xA;and&#xA;$TIBCO_ActivityDetails/root/Starter[(type='JMSQueueEventSource' or type='JMSTopicEventSource') and&#xA;(contains(rawConfig, '&lt;acknowledgeMode>2&lt;/acknowledgeMode>') or contains(rawConfig, '&lt;acknowledgeMode>23&lt;/acknowledgeMode>') or contains(rawConfig, '&lt;acknowledgeMode>24&lt;/acknowledgeMode>'))&#xA;] and&#xA;not($TIBCO_ActivityDetails/root/Activities[type='ConfirmActivity'])">
                <ns6:ChecklistValidation>
                    <xsl:if test="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='6']/ns8:ChecklistNumber">
                        <ns6:ChecklistNumber>
                            <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='6']/ns8:ChecklistNumber"/>
                        </ns6:ChecklistNumber>
                    </xsl:if>
                    <xsl:if test="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='6']/ns8:ChecklistName">
                        <ns6:ChecklistName>
                            <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='6']/ns8:ChecklistName"/>
                        </ns6:ChecklistName>
                    </xsl:if>
                    <ns6:Checklist>
                        <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='6']/ns8:ChecklistDetail"/>
                    </ns6:Checklist>
                    <ns6:Validation>
                        <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='6']/ns8:Severity"/>
                    </ns6:Validation>
                    <ns6:Remarks>
                        <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='6']/ns8:Remarks"/>
                    </ns6:Remarks>
                    <ns6:Process>
                        <xsl:value-of select="$Start/pfx4:root/ns1:FileInfo[1]/ns1:filePath"/>
                    </ns6:Process>
                    <ns6:Activity>
                        <xsl:value-of select="$TIBCO_ActivityDetails/root/Starter[($TIBCO_ActivityDetails/root/Starter/type='JMSQueueEventSource' or $TIBCO_ActivityDetails/root/Starter/type='JMSTopicEventSource')]/name"/>
                    </ns6:Activity>
                    <ns6:ReviewAccepted>
                        <xsl:value-of select="&quot;&quot;"/>
                    </ns6:ReviewAccepted>
                    <ns6:DeveloperComment>
                        <xsl:value-of select="&quot;&quot;"/>
                    </ns6:DeveloperComment>
                </ns6:ChecklistValidation>
            </xsl:if>
            <xsl:if test="{--CheckList7&#x9;Checklist_JMSHardcodedTimeout--}&#xA;$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='7']/ns8:Enabled= 'Yes'">
                <xsl:for-each select="$TIBCO_ActivityDetails/root/Starter[(type='JMSQueueEventSource' or type='JMSTopicEventSource') and&#xA;not(contains(rawConfig, '&lt;receiverTimeout>%%'))&#xA;]">
                    <ns6:ChecklistValidation>
                        <xsl:if test="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='7']/ns8:ChecklistNumber">
                            <ns6:ChecklistNumber>
                                <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='7']/ns8:ChecklistNumber"/>
                            </ns6:ChecklistNumber>
                        </xsl:if>
                        <xsl:if test="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='7']/ns8:ChecklistName">
                            <ns6:ChecklistName>
                                <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='7']/ns8:ChecklistName"/>
                            </ns6:ChecklistName>
                        </xsl:if>
                        <ns6:Checklist>
                            <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='7']/ns8:ChecklistDetail"/>
                        </ns6:Checklist>
                        <ns6:Validation>
                            <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='7']/ns8:Severity"/>
                        </ns6:Validation>
                        <ns6:Remarks>
                            <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='7']/ns8:Remarks"/>
                        </ns6:Remarks>
                        <ns6:Process>
                            <xsl:value-of select="$Start/pfx4:root/ns1:FileInfo[1]/ns1:filePath"/>
                        </ns6:Process>
                        <ns6:Activity>
                            <xsl:value-of select="current()/name"/>
                        </ns6:Activity>
                        <ns6:ReviewAccepted>
                            <xsl:value-of select="&quot;&quot;"/>
                        </ns6:ReviewAccepted>
                        <ns6:DeveloperComment>
                            <xsl:value-of select="&quot;&quot;"/>
                        </ns6:DeveloperComment>
                    </ns6:ChecklistValidation>
                </xsl:for-each>
            </xsl:if>
            <xsl:if test="{--CheckList8&#x9;Checklist_JMSHardcodedDestination--}&#xA;$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='8']/ns8:Enabled= 'Yes'">
                <xsl:for-each select="$TIBCO_ActivityDetails/root/Starter[(type='JMSQueueEventSource' or type='JMSTopicEventSource') and&#xA;not(contains(rawConfig, '&lt;destination>%%'))&#xA;]">
                    <ns6:ChecklistValidation>
                        <xsl:if test="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='8']/ns8:ChecklistNumber">
                            <ns6:ChecklistNumber>
                                <xsl:value-of select="$GetVariable/ns8:root/ns8:Checklist[ns8:ChecklistNumber='8']/ns8:ChecklistNumber"/>
                            </ns6:ChecklistNumber>
                        </xsl:if>
