truist.authz.policy.applied=true
truist.authz.policy.version=3.0.0

truist.mTLS-AuthZ.policy.applied=true
truist.mTLS-AuthZ.policy.version=3.0.0

truist.jsonencdec.policy.version=3.0.0
truist.wssencdec.policy.version=3.0.0

# Noname Policy
noname.policy.version=5.1.0

# Header Removal Policy
headerremoval.policy.version=1.1.2

assetType=rest
