    <pd:startX>101</pd:startX>
    <pd:startY>624</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>807</pd:endX>
    <pd:endY>628</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1628226061687</pd:targetNamespace>
    <pd:activity name="LogDEBUG">
        <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
        <pd:resourceType>ae.activities.log</pd:resourceType>
        <pd:x>701</pd:x>
        <pd:y>221</pd:y>
        <config>
            <role>DEBUG</role>
            <suppressHeader>true</suppressHeader>
        </config>
        <pd:inputBindings>
            <ns:ActivityInput>
                <message>
                    <xsl:value-of select="$Map-Data/log/msg"/>
                </message>
                <msgCode>
                    <xsl:value-of select="$Map-Data/log/msgCode"/>
                </msgCode>
            </ns:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="LogINFO">
