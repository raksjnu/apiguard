            <pd:inputBindings>
                <xsl:variable name="varProjectLocation" select="$tibcoProjectFolder/tibcoProjectFolder"/>
                <xsl:variable name="varProjectName" select="tib:substring-after-last(tib:substring-before-last($tibcoProjectFolder/tibcoProjectFolder, '\'), '\')"/>
