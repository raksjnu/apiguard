                        </ns3:path>
                        <ns3:mulePath>
                            <xsl:value-of select="concat(tib:substring-before-last($_globalVariables/ns:GlobalVariables/Tibco2MuleAccelerator/Input/TibcoRepoLocation, '\'),'\','MigrationOutput',$_globalVariables/ns:GlobalVariables/Tibco2MuleAccelerator/Input/muleProjecLocation,&quot;\&quot;,$varProjectName,&quot;\&quot;)"/>
