<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns6="www.tibco.com/plugin/Sleep" xmlns:ns5="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns="http://xmlns.example.com/1628143989205" xmlns:ns2="http://xmlns.example.com/unique/default/namespace/1134438639123" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:ns1="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:ns4="http://www.tibco.com/pe/EngineTypes" xmlns:ns3="http://www.tibco.com/pe/DeployedVarsType">
    <pd:name>Service/orderServiceFILE.process</pd:name>
    <pd:startName>File Poller</pd:startName>
    <pd:startX>0</pd:startX>
    <pd:startY>0</pd:startY>
    <pd:returnBindings/>
    <pd:starter name="File Poller">
        <pd:type>com.tibco.plugin.file.FileEventSource</pd:type>
        <pd:resourceType>ae.activities.FileEventSourceResource</pd:resourceType>
        <pd:x>33</pd:x>
        <pd:y>81</pd:y>
        <config>
            <pollInterval>5</pollInterval>
            <createEvent>true</createEvent>
            <modifyEvent>true</modifyEvent>
            <deleteEvent>true</deleteEvent>
            <mode>files-and-directories</mode>
            <encoding>text</encoding>
            <sortby>File Name</sortby>
            <sortorder>descending</sortorder>
            <fileName>%%Connections/FILE/inDir%%%%Connections/FILE/fileName%%</fileName>
            <includeCurrent>true</includeCurrent>
        </config>
        <pd:inputBindings/>
    </pd:starter>
    <pd:endName>End</pd:endName>
    <pd:endX>1115</pd:endX>
    <pd:endY>70</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1742402679731</pd:targetNamespace>
    <pd:activity name="Parse Data">
        <pd:type>com.tibco.plugin.parse.ParseActivity</pd:type>
        <pd:resourceType>ae.activities.ParseActivity</pd:resourceType>
        <pd:x>233</pd:x>
        <pd:y>79</pd:y>
        <config>
            <InputType>String</InputType>
            <Encoding>ASCII</Encoding>
            <ParseSharedConfig>/DataFormat/OrderServiceDataFormat.sharedparse</ParseSharedConfig>
        </config>
        <pd:inputBindings>
            <Input>
                <text>
                    <xsl:value-of select="$File-Poller/ns1:EventSourceOuputTextClass/fileContent/textContent"/>
                </text>
                <noOfRecords>
                    <xsl:value-of select="-1"/>
                </noOfRecords>
            </Input>
        </pd:inputBindings>
    </pd:activity>
    <pd:group name="Group">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>300</pd:x>
        <pd:y>1</pd:y>
        <pd:width>143</pd:width>
        <pd:height>140</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>inputLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:over>$Parse-Data/Output/Rows/root</pd:over>
            <pd:iterationElementSlot>customers</pd:iterationElementSlot>
            <pd:indexSlot>i</pd:indexSlot>
            <pd:activityOutputName/>
            <pd:accumulateOutput>false</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>createOrder</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>createOrder</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="createOrder">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>361</pd:x>
            <pd:y>78</pd:y>
            <config>
                <processName>/BusinessProcess/createOrderDummy.process</processName>
            </config>
            <pd:inputBindings>
                <ns:orderRequest>
                    <order>
                        <ns2:customer>
                            <ns2:customerid>
                                <xsl:value-of select="$customers/root/customerid"/>
                            </ns2:customerid>
                            <ns2:firstname>
                                <xsl:value-of select="$customers/root/firstname"/>
                            </ns2:firstname>
                            <ns2:lastname>
                                <xsl:value-of select="$customers/root/lastname"/>
                            </ns2:lastname>
                            <ns2:age>
                                <xsl:value-of select="$customers/root/age"/>
                            </ns2:age>
                            <ns2:email>
                                <xsl:value-of select="$customers/root/email"/>
                            </ns2:email>
                            <ns2:city>
                                <xsl:value-of select="$customers/root/city"/>
                            </ns2:city>
                            <ns2:zipcode>
                                <xsl:value-of select="$customers/root/zipcode"/>
                            </ns2:zipcode>
                            <ns2:transactionstatus>
                                <xsl:value-of select="$customers/root/transactionstatus"/>
                            </ns2:transactionstatus>
                        </ns2:customer>
                    </order>
                </ns:orderRequest>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:activity name="Catch">
        <pd:type>com.tibco.pe.core.CatchActivity</pd:type>
        <pd:resourceType>ae.activities.catch</pd:resourceType>
        <pd:x>1310</pd:x>
        <pd:y>66</pd:y>
        <pd:handler>true</pd:handler>
        <config>
            <catchAll>true</catchAll>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="LogandErrorHandler">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>1204</pd:x>
        <pd:y>67</pd:y>
        <config>
            <processName>/BusinessProcess/LogandErrorHandlerDummy.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <ns2:logLevel>
                    <xsl:value-of select="$_globalVariables/ns3:GlobalVariables/LogLevels/LOG_ERROR"/>
                </ns2:logLevel>
                <ns2:processCtx>
                    <ns2:processId>
                        <xsl:value-of select="$_processContext/ns4:ProcessContext/ProcessId"/>
                    </ns2:processId>
                    <ns2:projectName>
                        <xsl:value-of select="$_processContext/ns4:ProcessContext/ProjectName"/>
                    </ns2:projectName>
                    <ns2:ProcessName>
                        <xsl:value-of select="'/Common/RetryProcessErrors'"/>
                    </ns2:ProcessName>
                </ns2:processCtx>
                <ns2:description>
                    <xsl:value-of select="'exception handling activated'"/>
                </ns2:description>
                <ns2:ExceptionData>
                    <ns2:Message>
                        <xsl:value-of select="$_error/ns4:ErrorReport/Msg"/>
                    </ns2:Message>
                    <ns2:StackTrace>
                        <xsl:value-of select="$_error/ns4:ErrorReport/ProcessStack"/>
                    </ns2:StackTrace>
                    <ns2:ErrorData>
                        <xsl:value-of select="$_error/ns4:ErrorReport/Data/*"/>
                    </ns2:ErrorData>
                </ns2:ExceptionData>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Log">
        <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
        <pd:resourceType>ae.activities.log</pd:resourceType>
        <pd:x>235</pd:x>
        <pd:y>209</pd:y>
        <config>
            <role>User</role>
        </config>
        <pd:inputBindings>
            <ns5:ActivityInput>
                <message>
                    <xsl:value-of select="concat(&quot;File Poller Order Service Started with counter value =&quot;, $SetVariable/root/counter)"/>
                </message>
            </ns5:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Log-1">
        <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
        <pd:resourceType>ae.activities.log</pd:resourceType>
        <pd:x>1113</pd:x>
        <pd:y>256</pd:y>
        <config>
            <role>User</role>
        </config>
        <pd:inputBindings>
            <ns5:ActivityInput>
                <message>
                    <xsl:value-of select="&quot;File Poller Order Service Terminated&quot;"/>
                </message>
            </ns5:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Copy File">
        <pd:type>com.tibco.plugin.file.FileCopyActivity</pd:type>
        <pd:resourceType>ae.activities.FileCopyActivity</pd:resourceType>
        <pd:x>374</pd:x>
        <pd:y>254</pd:y>
        <config>
            <createMissingDirectories>true</createMissingDirectories>
            <overwrite>true</overwrite>
        </config>
        <pd:inputBindings>
            <ns1:CopyActivityConfig>
                <fromFileName>
                    <xsl:value-of select="$File-Poller/ns1:EventSourceOuputTextClass/fileInfo/fullName"/>
                </fromFileName>
                <toFileName>
                    <xsl:value-of select="concat($_globalVariables/ns3:GlobalVariables/Connections/FILE/outDir,$_globalVariables/ns3:GlobalVariables/Connections/FILE/fileName)"/>
                </toFileName>
            </ns1:CopyActivityConfig>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Rename File">
        <pd:type>com.tibco.plugin.file.FileRenameActivity</pd:type>
        <pd:resourceType>ae.activities.FileRenameActivity</pd:resourceType>
        <pd:x>491</pd:x>
        <pd:y>255</pd:y>
        <config>
            <createMissingDirectories>true</createMissingDirectories>
        </config>
        <pd:inputBindings>
            <ns1:RenameActivityConfig>
                <fromFileName>
                    <xsl:value-of select="concat($_globalVariables/ns3:GlobalVariables/Connections/FILE/outDir,$_globalVariables/ns3:GlobalVariables/Connections/FILE/fileName)"/>
                </fromFileName>
                <toFileName>
                    <xsl:value-of select="concat($_globalVariables/ns3:GlobalVariables/Connections/FILE/outDir,$_globalVariables/ns3:GlobalVariables/Connections/FILE/fileName,&quot;-&quot;,tib:format-dateTime(&quot;yyyyMMddHHmmss&quot;, current-dateTime()))"/>
                </toFileName>
            </ns1:RenameActivityConfig>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Remove File">
        <pd:type>com.tibco.plugin.file.FileRemoveActivity</pd:type>
        <pd:resourceType>ae.activities.FileRemoveActivity</pd:resourceType>
        <pd:x>730</pd:x>
        <pd:y>212</pd:y>
        <config/>
        <pd:inputBindings>
            <ns1:RemoveActivityInputClass>
                <fileName>
                    <xsl:value-of select="concat($_globalVariables/ns3:GlobalVariables/Connections/FILE/inDir,$_globalVariables/ns3:GlobalVariables/Connections/FILE/fileName)"/>
                </fileName>
            </ns1:RemoveActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="List Files">
        <pd:type>com.tibco.plugin.file.ListFilesActivity</pd:type>
        <pd:resourceType>ae.activities.ListFilesActivity</pd:resourceType>
        <pd:x>821</pd:x>
        <pd:y>214</pd:y>
        <config>
            <mode>files-and-directories</mode>
        </config>
        <pd:inputBindings>
            <ns1:ListFilesActivityConfig>
                <fileName>
                    <xsl:value-of select="$_globalVariables/ns3:GlobalVariables/Connections/FILE/outDir"/>
                </fileName>
            </ns1:ListFilesActivityConfig>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Write File">
        <pd:type>com.tibco.plugin.file.FileWriteActivity</pd:type>
        <pd:resourceType>ae.activities.FileWriteActivity</pd:resourceType>
        <pd:x>823</pd:x>
        <pd:y>314</pd:y>
        <config>
            <encoding>text</encoding>
            <compressFile>None</compressFile>
            <append>true</append>
            <createMissingDirectories>true</createMissingDirectories>
        </config>
        <pd:inputBindings>
            <ns1:WriteActivityInputTextClass>
                <fileName>
                    <xsl:value-of select="concat($_globalVariables/ns3:GlobalVariables/Connections/FILE/outDir, &quot;Log.txt&quot;)"/>
                </fileName>
                <textContent>
                    <xsl:value-of select="concat(&quot;log file updated at &quot;, current-dateTime(), &quot;&#xA;&quot;)"/>
                </textContent>
            </ns1:WriteActivityInputTextClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Null">
        <pd:type>com.tibco.plugin.timer.NullActivity</pd:type>
        <pd:resourceType>ae.activities.null</pd:resourceType>
        <pd:x>592</pd:x>
        <pd:y>258</pd:y>
        <config/>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="Sleep">
        <pd:type>com.tibco.plugin.timer.SleepActivity</pd:type>
        <pd:resourceType>ae.activities.sleep</pd:resourceType>
        <pd:x>728</pd:x>
        <pd:y>314</pd:y>
        <config/>
        <pd:inputBindings>
            <ns6:SleepInputSchema>
                <IntervalInMillisec>
                    <xsl:value-of select="1000"/>
                </IntervalInMillisec>
            </ns6:SleepInputSchema>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="GetVariable">
        <pd:type>com.tibco.pe.core.GetSharedVariableActivity</pd:type>
        <pd:resourceType>ae.activities.getSharedVariable</pd:resourceType>
        <pd:x>39</pd:x>
        <pd:y>208</pd:y>
        <config>
            <variableConfig>/SharedVariables/SharedVariable_counter.sharedvariable</variableConfig>
        </config>
        <pd:inputBindings/>
    </pd:activity>
    <pd:activity name="SetVariable">
        <pd:type>com.tibco.pe.core.SetSharedVariableActivity</pd:type>
        <pd:resourceType>ae.activities.setSharedVariable</pd:resourceType>
        <pd:x>134</pd:x>
        <pd:y>208</pd:y>
        <config>
            <variableConfig>/SharedVariables/SharedVariable_counter.sharedvariable</variableConfig>
            <showResult>true</showResult>
        </config>
        <pd:inputBindings>
            <root>
                <counter>
                    <xsl:value-of select="$GetVariable/root/counter +1"/>
                </counter>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>File Poller</pd:from>
        <pd:to>GetVariable</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Parse Data</pd:from>
        <pd:to>Group</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>Copy File</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Catch</pd:from>
        <pd:to>LogandErrorHandler</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>LogandErrorHandler</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log</pd:from>
        <pd:to>Parse Data</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log-1</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Copy File</pd:from>
        <pd:to>Rename File</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Remove File</pd:from>
        <pd:to>List Files</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>List Files</pd:from>
        <pd:to>Log-1</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Rename File</pd:from>
        <pd:to>Null</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Write File</pd:from>
        <pd:to>Log-1</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Null</pd:from>
        <pd:to>Remove File</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>1=2</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Null</pd:from>
        <pd:to>Sleep</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Sleep</pd:from>
        <pd:to>Write File</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>SetVariable</pd:from>
        <pd:to>Log</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>GetVariable</pd:from>
        <pd:to>SetVariable</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>