<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns2="http://xmlns.tibco.com/bw/activity/xml/render/bytesEnvelope/2003/05" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns1="http://www.tibco.com/pe/EngineTypes" xmlns:ns="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:pfx4="http://xmlns.example.com/unique/default/namespace/1134438639123" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx3="http://www.tibco.com/CNV/SharedResources/Schemas" xmlns:pfx2="http://www.tibco.com/pe/DeployedVarsType" xmlns:pfx="http://ns.bombardier.com/soahrt/logging" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <xsd:import namespace="http://www.tibco.com/CNV/SharedResources/Schemas" schemaLocation="/SharedResources/Schemas/CNVDataSchema.xsd"/>
    <xsd:import namespace="http://xmlns.example.com/unique/default/namespace/1134438639123"/>
    <xsd:import namespace="http://xmlns.example.com/unique/default/namespace/1134438639123" schemaLocation="/Schema/LogError.xsd"/>
    <pd:name>BusinessProcess/LogandErrorHandler.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="root" type="pfx4:LogType"/>
    </pd:startType>
    <pd:startX>101</pd:startX>
    <pd:startY>624</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>807</pd:endX>
    <pd:endY>628</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1628226061687</pd:targetNamespace>
    <pd:activity name="LogDEBUG">
        <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
        <pd:resourceType>ae.activities.log</pd:resourceType>
        <pd:x>701</pd:x>
        <pd:y>221</pd:y>
        <config>
            <role>DEBUG</role>
            <suppressHeader>true</suppressHeader>
        </config>
        <pd:inputBindings>
            <ns:ActivityInput>
                <message>
                    <xsl:value-of select="$Map-Data/log/msg"/>
                </message>
                <msgCode>
                    <xsl:value-of select="$Map-Data/log/msgCode"/>
                </msgCode>
            </ns:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="LogINFO">
        <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
        <pd:resourceType>ae.activities.log</pd:resourceType>
        <pd:x>585</pd:x>
        <pd:y>292</pd:y>
        <config>
            <role>INFO</role>
            <suppressHeader>true</suppressHeader>
        </config>
        <pd:inputBindings>
            <ns:ActivityInput>
                <message>
                    <xsl:value-of select="$Map-Data/log/msg"/>
                </message>
                <msgCode>
                    <xsl:value-of select="$Map-Data/log/msgCode"/>
                </msgCode>
            </ns:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="LogWARN">
        <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
        <pd:resourceType>ae.activities.log</pd:resourceType>
        <pd:x>483</pd:x>
        <pd:y>344</pd:y>
        <config>
            <role>WARN</role>
            <suppressHeader>true</suppressHeader>
        </config>
        <pd:inputBindings>
            <ns:ActivityInput>
                <message>
                    <xsl:value-of select="$Map-Data/log/msg"/>
                </message>
                <msgCode>
                    <xsl:value-of select="$Map-Data/log/msgCode"/>
                </msgCode>
            </ns:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="LogERROR">
        <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
        <pd:resourceType>ae.activities.log</pd:resourceType>
        <pd:x>186</pd:x>
        <pd:y>418</pd:y>
        <config>
            <role>ERROR</role>
            <suppressHeader>true</suppressHeader>
        </config>
        <pd:inputBindings>
            <ns:ActivityInput>
                <message>
                    <xsl:value-of select="$Map-Data/log/msg"/>
                </message>
                <msgCode>
                    <xsl:value-of select="$Map-Data/log/msgCode"/>
                </msgCode>
            </ns:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Map Data">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>96</pd:x>
        <pd:y>148</pd:y>
        <config>
            <element>
                <xsd:element name="log">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="msg" type="xsd:string"/>
                            <xsd:element name="msgCode" type="xsd:string"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </element>
        </config>
        <pd:inputBindings>
            <log>
                <xsl:variable name="var" select="number($Start/root/pfx4:logLevel)"/>
                <msg>
                    <xsl:value-of select="concat(&#xA;&#x9;tib:tokenize(&quot;DEBUG|INFO|WARN|ERROR&quot;, &quot;|&quot;)[$var], &quot; | &quot;,&#xA;&#x9;$Start/root/pfx4:processCtx/pfx4:projectName, &quot; | &quot;,&#xA;&#x9;$Start/root/pfx4:processCtx/pfx4:ProcessName, $Start/root/pfx4:processCtx/pfx4:processId , &quot; | &quot;,&#xA;&#x9;$Start/root/pfx4:description)"/>
                </msg>
                <msgCode>
                    <xsl:value-of select="&quot;&quot;"/>
                </msgCode>
            </log>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="insertProcessErrors">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>362</pd:x>
        <pd:y>418</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/Connections/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <statement>insert into process_errors (processid ,  process_name ,  ERROR_DESCRIPTION ,errortime )
values (?,?,?,?)
</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>process_id</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>process_name</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>error_description</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>errortime</parameterName>
                    <dataType>TIMESTAMP</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <process_id>
                    <xsl:value-of select="$Start/root/pfx4:processCtx/pfx4:processId"/>
                </process_id>
                <process_name>
                    <xsl:value-of select="$Start/root/pfx4:processCtx/pfx4:projectName"/>
                </process_name>
                <error_description>
                    <xsl:choose>
                        <xsl:when test="exists($Start/root/pfx4:ExceptionData/pfx4:Message)">
                            <xsl:value-of select="$Start/root/pfx4:ExceptionData/pfx4:Message"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </error_description>
                <errortime>
                    <xsl:value-of select="current-dateTime()"/>
                </errortime>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="insertFail">
        <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
        <pd:resourceType>ae.activities.log</pd:resourceType>
        <pd:x>362</pd:x>
        <pd:y>551</pd:y>
        <config>
            <role>User</role>
        </config>
        <pd:inputBindings>
            <ns:ActivityInput>
                <message>
                    <xsl:value-of select="concat('ERROR ********', $Start/root/pfx4:processCtx/pfx4:ProcessName, &quot; -- &quot;,   $Start/root/pfx4:ExceptionData/pfx4:Message)"/>
                </message>
            </ns:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>LogWARN</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>LogINFO</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Straight</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>LogDEBUG</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Map Data</pd:to>
        <pd:xpathDescription>Logging Enabled</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$_globalVariables/pfx2:GlobalVariables/LogLevels/LoggingEnabled="true"  and  $Start/root/pfx4:logLevel &gt;= $_globalVariables/pfx2:GlobalVariables/LogLevels/LOG_LEVEL</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Map Data</pd:from>
        <pd:to>LogDEBUG</pd:to>
        <pd:xpathDescription>DEBUG</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-6908416</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Start/root/pfx4:logLevel=$_globalVariables/pfx2:GlobalVariables/LogLevels/LOG_DEBUG</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Map Data</pd:from>
        <pd:to>LogINFO</pd:to>
        <pd:xpathDescription>INFO</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16763905</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Start/root/pfx4:logLevel=$_globalVariables/pfx2:GlobalVariables/LogLevels/LOG_INFO</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Map Data</pd:from>
        <pd:to>LogWARN</pd:to>
        <pd:xpathDescription>WARN</pd:xpathDescription>
        <pd:lineType>Straight</pd:lineType>
        <pd:lineColor>-27136</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Start/root/pfx4:logLevel=$_globalVariables/pfx2:GlobalVariables/LogLevels/LOG_WARNING</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Map Data</pd:from>
        <pd:to>LogERROR</pd:to>
        <pd:xpathDescription>ERROR</pd:xpathDescription>
        <pd:lineType>Straight</pd:lineType>
        <pd:lineColor>-65536</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Start/root/pfx4:description</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>Map Data</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>One Bend</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>insertProcessErrors</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>insertProcessErrors</pd:from>
        <pd:to>insertFail</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-65485</pd:lineColor>
        <pd:conditionType>error</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>insertFail</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>LogERROR</pd:from>
        <pd:to>insertProcessErrors</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>