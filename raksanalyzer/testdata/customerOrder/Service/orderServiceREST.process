            <eventSource>HTTP Receiver</eventSource>
            <RestService>
                <InnerService>
                    <ns0:application xmlns:ns0="http://wadl.dev.java.net/2009/02" name="Application"/>
                </InnerService>
                <OuterBinding>
                    <Binding path="http://localhost:8085/orderCreateAPI/customerDetails/CreateCustomer" process="/BusinessProcess/createOrderREST.process">
                        <RESTResponseHeader>
                            <xsd:element name="Headers">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="StatusLine" type="xsd:string" minOccurs="0"/>
                                        <xsd:element name="Content-Type" type="xsd:string" minOccurs="0"/>
                                        <xsd:element name="Location" type="xsd:string" minOccurs="0"/>
                                        <xsd:element name="Set-Cookie" type="xsd:string" minOccurs="0"/>
                                        <xsd:element name="Pragma" type="xsd:string" minOccurs="0"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </RESTResponseHeader>
                        <Input-Binding xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
                            <xsl:template xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns="http://xmlns.example.com/1628143989205" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns1="http://xmlns.example.com/unique/default/namespace/1134438639123" match="/*">
                                <ns:orderRequest>
                                    <order>
                                        <ns1:customer>
                                            <ns1:customerid>
                                                <xsl:value-of select="$Input-Binding/params/header/customerid"/>
                                            </ns1:customerid>
                                            <ns1:firstname>
                                                <xsl:value-of select="$Input-Binding/params/header/firstname"/>
                                            </ns1:firstname>
                                            <ns1:lastname>
                                                <xsl:value-of select="$Input-Binding/params/header/lastname"/>
                                            </ns1:lastname>
                                            <ns1:age>
                                                <xsl:value-of select="$Input-Binding/params/header/age"/>
                                            </ns1:age>
                                            <ns1:email>
                                                <xsl:value-of select="$Input-Binding/params/header/email"/>
                                            </ns1:email>
                                            <ns1:city>
                                                <xsl:value-of select="$Input-Binding/params/header/city"/>
                                            </ns1:city>
                                            <ns1:zipcode>
                                                <xsl:value-of select="$Input-Binding/params/header/zipcode"/>
                                            </ns1:zipcode>
                                            <ns1:transactionstatus>
                                                <xsl:value-of select="$Input-Binding/params/header/transactionstatus"/>
                                            </ns1:transactionstatus>
                                        </ns1:customer>
                                    </order>
                                </ns:orderRequest>
                            </xsl:template>
                        </Input-Binding>
                        <Output-Binding xmlns:pfx="http://xmlns.example.com/1628143989205" xmlns:pfx2="http://xmlns.example.com/1628144235184" xmlns:ns="http://xmlns.example.com/unique/default/namespace/1134438639123" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns0="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:ns1="http://schemas.tibco.com/bw/plugins/xml/5.0/xmlExceptions" xmlns:ns4="http://www.tibco.com/schemas/customerOrder/Schema/Schema1.xsd" xmlns:ns3="http://www.tibco.com/pe/EngineTypes" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns6="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:ns5="http://www.tibco.com/pe/DeployedVarsType" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:pd="http://xmlns.tibco.com/bw/process/2003">
                            <xsl:template xmlns:pfx="http://xmlns.example.com/1628143989205" xmlns:pfx2="http://xmlns.example.com/1628144235184" xmlns:ns="http://xmlns.example.com/unique/default/namespace/1134438639123" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns0="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:ns1="http://schemas.tibco.com/bw/plugins/xml/5.0/xmlExceptions" xmlns:ns4="http://www.tibco.com/schemas/customerOrder/Schema/Schema1.xsd" xmlns:ns3="http://www.tibco.com/pe/EngineTypes" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns6="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:ns5="http://www.tibco.com/pe/DeployedVarsType" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:pd="http://xmlns.tibco.com/bw/process/2003">
                                <RESTResponse>
                                    <asciiContent>
                                        <xsl:value-of select="$End/Status/StatusDetails"/>
                                    </asciiContent>
                                    <Headers>
                                        <Content-Type>
                                            <xsl:value-of select="&quot;application/json&quot;"/>
                                        </Content-Type>
                                    </Headers>
                                </RESTResponse>
                            </xsl:template>
                        </Output-Binding>
                    </Binding>
                </OuterBinding>
            </RestService>
        </config>
        <pd:inputBindings>
            <ns0:ActivityInputClass xmlns:ns0="http://www.tibco.com/namespaces/tnt/plugins/json">
                <tmp>
                    <xsl:value-of select="$HTTP-Receiver/ProcessStarterOutput/Method"/>
                </tmp>
            </ns0:ActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Log">
