                    <ns:transactionstatus>
                        <xsl:value-of select="$Start/pfx:orderRequest/order/ns:customer/ns:transactionstatus"/>
                    </ns:transactionstatus>
                </ns:customer>
            </order>
        </pfx:orderResponse>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
