    <pd:activity name="create_mule-projectXML">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:description/>
        <pd:x>134</pd:x>
        <pd:y>120</pd:y>
        <config>
            <element>
                <xsd:element name="root">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="mule-projectXML" type="xsd:string" minOccurs="0"/>
                            <xsd:element name="pomXML" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </element>
        </config>
        <pd:inputBindings>
            <xsl:variable name="varHead" select="$_globalVariables/ns:GlobalVariables/Tibco2MuleAccelerator/MuleVaribales/mule-projectHead"/>
            <xsl:variable name="varTail" select="$_globalVariables/ns:GlobalVariables/Tibco2MuleAccelerator/MuleVaribales/mule-projectTail"/>
            <xsl:variable name="varName" select="$Start/pfx4:root/ns1:ProjectInfo[1]/ns1:name"/>
            <xsl:variable name="varDescription" select="concat( $_globalVariables/ns:GlobalVariables/Tibco2MuleAccelerator/Messages/MuleProjectDescription, &quot; for Tibco project &quot;, $Start/pfx4:root/ns1:ProjectInfo[1]/ns1:path)"/>
            <root>
                <mule-projectXML>
                    <xsl:value-of select="concat($varHead, &quot;&lt;name>&quot;, $varName, &quot;&lt;/name>&quot;, &quot;&lt;description>&quot;, $varDescription, &quot;&lt;/description>&quot;, $_globalVariables/ns:GlobalVariables/Tibco2MuleAccelerator/MuleVaribales/mule-projectTail)"/>
                </mule-projectXML>
                <pomXML>
                    <xsl:value-of select="concat($_globalVariables/ns:GlobalVariables/Tibco2MuleAccelerator/Messages/pomXML_part1, $varName,$_globalVariables/ns:GlobalVariables/Tibco2MuleAccelerator/Messages/pomXML_part2,$varName, $_globalVariables/ns:GlobalVariables/Tibco2MuleAccelerator/Messages/pomXML_part3)"/>
                </pomXML>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="CreateFolderList">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
