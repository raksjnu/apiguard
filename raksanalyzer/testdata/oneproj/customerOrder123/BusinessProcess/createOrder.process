                    </ns:age>
                    <ns:email>
                        <xsl:value-of select="$Start/pfx:orderRequest/order/ns:customer/ns:email"/>
                    </ns:email>
                    <ns:city>
                        <xsl:value-of select="$Start/pfx:orderRequest/order/ns:customer/ns:city"/>
                    </ns:city>
                    <ns:zipcode>
