        <pd:inputBindings>
            <root>
                <xsl:variable name="var1" select="$TIBCO_XML/child::*/*"/>
                <xsl:variable name="varActivityCount" select="if(exists($var1[local-name(.)='starter']) )&#xA;then&#xA;count($var1[local-name(.)='activity'])+1&#xA;else&#xA;count($var1[local-name(.)='activity'])"/>
                <name>
                    <xsl:value-of select="$var1[local-name(.)='name']"/>
                </name>
                <xsl:if test="$var1[local-name(.)='startName']">
                    <startName>
                        <xsl:value-of select="$var1[local-name(.)='startName']"/>
                    </startName>
                </xsl:if>
                <xsl:choose>
                    <xsl:when test="$var1[local-name(.)='starter']">
                        <Starter>
                            <xsl:if test="$TIBCO_XML/child::*/*[local-name(.)='starter']/child::*/*[local-name(.)='name']">
                                <name>
                                    <xsl:value-of select="$TIBCO_XML/child::*/*[local-name(.)='starter']/child::*/*[local-name(.)='name']"/>
                                </name>
                            </xsl:if>
                            <type>
                                <xsl:value-of select="tib:substring-after-last($var1[local-name(.)='starter']/child::pd:type,'.')"/>
                            </type>
                            <resourceType>
                                <xsl:value-of select="tib:substring-after-last($TIBCO_XML/child::*/child::pd:starter/child::pd:resourceType,'.')"/>
                            </resourceType>
                            <config>
                                <xsl:value-of select="tib:render-xml($var1[local-name(.)='starter']/config)"/>
                            </config>
                            <InputBinding>
                                <xsl:value-of select="tib:render-xml($var1[local-name(.)='starter']/pd:inputBindings)"/>
                            </InputBinding>
                        </Starter>
                    </xsl:when>
                    <xsl:otherwise>
                        <Starter>
                            <name>
                                <xsl:value-of select="'Start'"/>
                            </name>
                            <type>
                                <xsl:value-of select="'StartType'"/>
                            </type>
                            <resourceType>
                                <xsl:value-of select="'DefaultStartType'"/>
                            </resourceType>
                            <config>
                                <xsl:value-of select="'N/A'"/>
                            </config>
                            <InputBinding>
                                <xsl:value-of select="'N/A'"/>
                            </InputBinding>
                        </Starter>
                    </xsl:otherwise>
                </xsl:choose>
                <xsl:for-each select="$var1[local-name(.)='activity' or local-name(.)='starter']">
                    <Activities>
                        <name>
                            <xsl:value-of select="current()/attribute::name[1]"/>
                        </name>
                        <type>
                            <xsl:value-of select="tib:substring-after-last(current()/child::pd:type, '.')"/>
                        </type>
                        <resourceType>
                            <xsl:value-of select="tib:substring-after-last(current()/child::pd:resourceType, '.')"/>
                        </resourceType>
                        <config>
                            <xsl:value-of select="substring-before(substring-after(tib:render-xml(current()/config), &quot;&lt;config>&quot;), &quot;&lt;/config>&quot;)&#xA;{--tib:render-xml(current()/config)--}"/>
                        </config>
                        <InputBinding>
                            <xsl:value-of select="{--substring-before(substring-after(tib:render-xml(current()/pd:inputBindings), &quot;&lt;root>&quot;), &quot;&lt;/root>&quot;)--}&#xA;tib:render-xml(current()/pd:inputBindings)"/>
                        </InputBinding>
                    </Activities>
                </xsl:for-each>
                <ActivityCount>
                    <xsl:value-of select="$varActivityCount"/>
                </ActivityCount>
            </root>
        </pd:inputBindings>
    </pd:activity>
